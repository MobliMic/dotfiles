execute pathogen#infect()
syntax enable 
filetype plugin indent on
set number
set background=dark
let g:solarized_termcolors=256
colorscheme solarized

let g:multi_cursor_exit_from_visual_mode = 0
let g:multi_cursor_exit_from_insert_mode = 0

set expandtab
set tabstop=4
set shiftwidth=4

map <F7> mzgg=G`z
